<%- include('../components/header.ejs') %>
    <!-- Begin Page Content -->
    <div class="container-fluid">
        <!-- Page Heading -->
        <h1 class="h3 mb-4 text-gray-800">Dashboard <sup> Admin</sup></h1>
        <% if (alert.message != '') { %>
            <div class="alert alert-<%= alert.status %>" role="alert">
            <%- alert.status == 'success' ?
            `<i class="fa fa-grin-hearts mr-2"></i>`:  
            `<i class="fa fa-smile-beam" ></i>`  
            %>
            <%= alert.message %>
            <!-- <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button> -->
            </div>
        <% } %>

        <div class="card-dashboard"></div>
    </div>
    <!-- /.container-fluid -->
<script>
 // timer
 $(document).ready(function(){
      const timerAlert = $('.alert').text();
      if (timerAlert !== ''){
        setTimeout(function(){
          $('.alert').fadeOut('slow');
        }, 4000);
      }
    });
</script>
<%- include('../components/bottom.ejs') %>
